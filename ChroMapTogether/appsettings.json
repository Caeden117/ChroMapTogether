{
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:5000"
      }
    }
  },
  "ChroMapTogether": {
    "RoomExpiryPeriod": 1,
    "RoomCodeLength": 5
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Overrides": {
        "Microsoft": "Information",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "File": {
      "Path": "log/ChroMapTogether-.log"
    }
  }
}
